<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Dashing.Cli</id>
	<version>$version$</version>
    <title>Dashing Cli</title>
	<authors>Polylytics</authors>
	<owners>Polylytics</owners>
	<projectUrl>https://github.com/Polylytics/dashing</projectUrl>
	<iconUrl>http://polylytics.com/dashing/dashing-icon.png</iconUrl>
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
	<description>Dashing.Cli is a command line tool for migrating database schemas</description>
	<copyright>© Polylytics Limited, 2018</copyright>
	<language>en-GB</language>
	<tags>dashing dapper orm sql mini-orm</tags>
    <packageTypes>
      <packageType name="DotNetCliTool" />
    </packageTypes>
    <dependencies>
      <group targetFramework=".NETCoreApp1.1">
        <dependency id="Dashing" version="$version$" exclude="Build,Analyzers" />
        <dependency id="McMaster.Extensions.CommandLineUtils" version="2.2.5" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.DependencyModel" version="1.1.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.PlatformAbstractions" version="1.1.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.NETCore.App" version="1.1.2" exclude="Build,Analyzers" />
        <dependency id="MySql.Data" version="6.10.6" exclude="Build,Analyzers" />
		<dependency id="Serilog.Sinks.Console" version="3.1.1" exclude="Build,Analyzers" />
        <dependency id="System.Data.SqlClient" version="4.4.2" exclude="Build,Analyzers" />
        <dependency id="System.Runtime.Loader" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="System.Xml.XmlDocument" version="4.3.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETCoreApp2.0">
        <dependency id="Dashing" version="$version$" exclude="Build,Analyzers" />
        <dependency id="McMaster.Extensions.CommandLineUtils" version="2.2.5" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.DependencyModel" version="1.1.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.PlatformAbstractions" version="1.1.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.NETCore.App" version="1.1.2" exclude="Build,Analyzers" />
        <dependency id="MySql.Data" version="6.10.6" exclude="Build,Analyzers" />
		<dependency id="Serilog.Sinks.Console" version="3.1.1" exclude="Build,Analyzers" />
        <dependency id="System.Data.SqlClient" version="4.4.2" exclude="Build,Analyzers" />
        <dependency id="System.Runtime.Loader" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="System.Xml.XmlDocument" version="4.3.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="$publishdir$\net46\**\*" target="tools/net46/" />
	<file src="_._" target="lib/net46/" />
	<file src="Init.ps1" target="tools\init.ps1" />
    <file src="$outputDir$netcoreapp1.1\dotnet-dash.dll" target="lib/netcoreapp1.1/dotnet-dash.dll" />
	<file src="$outputDir$netcoreapp1.1\dotnet-dash.runtimeconfig.json" target="lib/netcoreapp1.1/dotnet-dash.runtimeconfig.json" />
    <file src="$outputDir$netcoreapp2.0\dotnet-dash.dll" target="lib/netcoreapp2.0/dotnet-dash.dll" />
	<file src="$outputDir$netcoreapp2.0\dotnet-dash.runtimeconfig.json" target="lib/netcoreapp2.0/dotnet-dash.runtimeconfig.json" />
  </files>
</package>